{
  "name": "webpack-express-typescript",
  "version": "1.0.0",
  "description": "Easy starter project for writing modern Express Applications using TypeScript, and Webpack",
  "private": true,
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "node node_modules/webpack/bin/webpack.js --config config/webpack.config.js",
    "postinstall": "node node_modules/typings/dist/bin.js install --overwrite",
    "hot": "rm -rf out && webpack \"webpack/hot/poll?1000\" --watch --hot",
    "start": "npm run build && node build/main.bundle.js",
    "hot2": "webpack-dev-server --inline --hot"
  },
  "main": "",
  "keywords": [
    "Easy",
    "TypeScript",
    "Starter",
    "Template"
  ],
  "author": "Rick Hernandez",
  "license": "MIT",
  "devDependencies": {
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-livereload-plugin": "^0.4.0"
   },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "app-root-path": "^1.3.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "morgan": "^1.7.0"
  }
}