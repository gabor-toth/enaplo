{
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ES5",
		"noImplicitAny": false,
		"sourceMap": true,
		"outDir": "build",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true
	},
	"exclude": [
		"node_modules",
		"build",
		"config",
		"release"
	],
	"___types": [
		"node"
	],
	"compileOnSave": true
}